<h1> Hello Admin </h1>

<h3> List of Users </h3>

<% @users.each do |user| %>
	<p>
		<strong>Name:</strong> <%= user.full_name %>	
		<strong>Email:</strong> <%= user.email %>
	</p>
	<%= link_to "Edit User", edit_admin_user_path(user) %>
	<%= link_to "Delete User", admin_user_path(user), method: :delete, data: {confirm: "Are you sure?" } %>
<% end %>

<hr/>